{
  "name": "online-ide",
  "version": "1.0.0",
  "description": "An Online IDE that can compile excerpts from different programming languages.",
  "main": "build/index.js",
  "scripts": {
    "devStart": "nodemon -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "build": "npx tsc",
    "start": "node build/index.js",
    "init-lint": "npx eslint --init",
    "check-lint": "npx eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijays-hub/online-ide.git"
  },
  "keywords": [
    "nodejs",
    "child-process",
    "shell-command",
    "programming",
    "express",
    "mongodb",
    "typescript"
  ],
  "author": "Vijay S Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vijays-hub/online-ide/issues"
  },
  "homepage": "https://github.com/vijays-hub/online-ide#readme",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  }
}
